{
  "name": "react-type-mui",
  "version": "0.1.0",
  "description": " React + Typescript + MUI starter ",
  "main": "src/index.tsx",
  "scripts": {
    "start:dev": "webpack-cli serve --mode=development --env development --open",
    "build": "webpack --mode=production --env production --progress",
    "lint": "tsc --noEmit && eslint \"*/**/*.{js,ts,tsx}\"",
    "test": "jest --passWithNoTests",
    "lint-fix": "eslint --fix \"*/**/*.{js,ts,tsx}\"",
    "dev": "npm-run-all --parallel start:dev lint"
  },
  "author": "Lukasz Drazewski",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/dom": "^8.7.0",
    "@testing-library/react": "^11.0.4",
    "@testing-library/react-hooks": "^3.4.1",
    "@types/jest": "^26.0.10",
    "@types/node": "^16.9.6",
    "@types/react": "17.0.20",
    "@types/react-dom": "17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/webpack": "5.28.0",
    "@types/webpack-dev-server": "3.11.6",
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "@typescript-eslint/parser": "4.31.0",
    "dotenv": "^10.0.0",
    "dotenv-webpack": "^7.0.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "eslint-watch": "^6.0.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "6.3.3",
    "html-webpack-plugin": "5.3.2",
    "jest": "^26.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.4.0",
    "terser-webpack-plugin": "^4.1.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "typescript": "4.4.2",
    "url-loader": "^4.1.1",
    "webpack": "5.52.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "3.11.2"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.0",
    "@mui/material": "^5.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^5.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@pnp)/)"
    ]
  }
}
